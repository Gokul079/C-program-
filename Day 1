4

#include <stdio.h>
#include<string.h>

int main()
{
   char str[50];
   int index=0;
   fgets(str,sizeof(str),stdin);
   puts(str);
   while(str[index]!='\0')
   index++;
   printf("%d",index-1);

    return 0;
}

